{
    "name": "followgt-nomade",
    "version": "3.12.1",
    "author": "Wiilog Team",
    "description": "Follow your supply chain with GT Logistics.",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "wiis:generate:build-json": "node scripts/generate-build-json",
        "wiis:generate:dev-credentials": "node scripts/generate-dev-credentials",
        "wiis:generate:network-security-config": "node scripts/generate-network-security-config",
        "wiis:generate:network-security-config:prod": "node scripts/generate-network-security-config prod",
        "wiis:clean": "node scripts/rmdir www && ionic cordova resources android && ionic cordova platform add android && cordova clean",
        "wiis:build:prod": "yarn wiis:build --prod --release --buildConfig=build.json",
        "wiis:build": "node scripts/rmdir platforms && yarn wiis:generate:build-json && yarn wiis:generate:dev-credentials && yarn wiis:generate:network-security-config:prod && yarn wiis:clean && ionic cordova build android",
        "wiis:run": "yarn wiis:generate:dev-credentials && yarn wiis:generate:network-security-config && yarn wiis:clean && ionic cordova run android",
        "wiis:run:livereload": "yarn wiis:generate:dev-credentials && yarn wiis:generate:network-security-config && yarn wiis:clean && ionic cordova run android --livereload"
    },
    "dependencies": {
        "@angular/common": "~9.1.6",
        "@angular/core": "~9.1.6",
        "@angular/forms": "~9.1.6",
        "@angular/platform-browser": "~9.1.6",
        "@angular/platform-browser-dynamic": "~9.1.6",
        "@angular/router": "~9.1.6",
        "@ionic-native/app-version": "^5.25.0",
        "@ionic-native/barcode-scanner": "^5.25.0",
        "@ionic-native/camera": "^5.27.0",
        "@ionic-native/core": "^5.0.7",
        "@ionic-native/date-picker": "^5.27.0",
        "@ionic-native/network": "^5.25.0",
        "@ionic-native/splash-screen": "^5.0.0",
        "@ionic-native/sqlite": "^5.25.0",
        "@ionic-native/status-bar": "^5.0.0",
        "@ionic/angular": "^5.0.0",
        "@ionic/storage": "^2.2.0",
        "@types/hammerjs": "^2.0.36",
        "com-darryncampbell-cordova-plugin-intent": "^2.0.0",
        "cordova-android": "^8.1.0",
        "cordova-plugin-androidx-adapter": "^1.1.0",
        "cordova-plugin-app-version": "^0.1.9",
        "cordova-plugin-camera": "^4.1.0",
        "cordova-plugin-datepicker": "^0.9.3",
        "cordova-plugin-network-information": "^2.0.2",
        "cordova-sqlite-storage": "^5.0.0",
        "core-js": "^2.5.4",
        "hammerjs": "^2.0.8",
        "ionic-long-press": "^2.0.3",
        "ionic-selectable": "^4.7.1",
        "moment": "^2.25.3",
        "open-sans-fonts": "^1.6.2",
        "phonegap-plugin-barcodescanner": "^8.1.0",
        "rxjs": "~6.5.1",
        "semver": "^7.3.2",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.5",
        "@angular/cli": "9.1.7",
        "@angular/compiler": "~9.1.6",
        "@angular/compiler-cli": "~9.1.6",
        "@angular/language-service": "~9.1.6",
        "@ionic/angular-toolkit": "^2.1.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-ionic-keyboard": "^2.2.0",
        "cordova-plugin-ionic-webview": "^4.2.1",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-statusbar": "^2.4.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3"
    },
    "cordova": {
        "plugins": {
            "cordova-sqlite-storage": {},
            "phonegap-plugin-barcodescanner": {
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            },
            "cordova-plugin-app-version": {},
            "cordova-plugin-network-information": {},
            "com-darryncampbell-cordova-plugin-intent": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-ionic-webview": {
                "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
            },
            "cordova-plugin-ionic-keyboard": {},
            "cordova-plugin-androidx-adapter": {},
            "cordova-plugin-camera": {
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            },
            "cordova-plugin-datepicker": {}
        },
        "platforms": [
            "android"
        ]
    }
}
