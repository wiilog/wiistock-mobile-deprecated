{
    "name": "followgt-nomade",
    "version": "6.21.3",
    "author": "Wiilog Team",
    "description": "Follow your supply chain with GT Logistics.",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "wiis:generate:build-json": "node scripts/generate-build-json",
        "wiis:generate:dev-credentials": "node scripts/generate-dev-credentials",
        "wiis:generate:network-security-config": "node scripts/generate-network-security-config",
        "wiis:generate:network-security-config:prod": "node scripts/generate-network-security-config prod",
        "wiis:clean": "node scripts/rmdir www && yarn ionic cordova resources android && yarn ionic cordova platform add android && yarn cordova clean",
        "wiis:build:prod": "yarn wiis:build --prod --release --buildConfig=build.json",
        "wiis:build": "node scripts/rmdir platforms && yarn wiis:generate:build-json && yarn wiis:generate:dev-credentials && yarn wiis:generate:network-security-config:prod && yarn wiis:clean && ionic cordova build android",
        "wiis:run": "yarn wiis:generate:dev-credentials && node scripts/get-local-address.js && yarn wiis:generate:network-security-config && yarn wiis:clean && yarn ionic cordova run android",
        "wiis:run:livereload": "yarn wiis:generate:dev-credentials && node scripts/get-local-address.js && yarn wiis:generate:network-security-config && yarn wiis:clean && yarn ionic cordova run android --livereload",
        "watch": "yarn wiis:run:livereload"
    },
    "dependencies": {
